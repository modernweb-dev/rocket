{
  "name": "@rocket/search",
  "version": "0.5.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Search Preset for Rocket",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modernweb-dev/rocket.git",
    "directory": "packages/search"
  },
  "author": "Modern Web <hello@modern-web.dev> (https://modern-web.dev/)",
  "homepage": "https://rocket.modern-web.dev/docs/presets/search/",
  "main": "./node.js",
  "type": "module",
  "exports": {
    ".": "./src/index.node.js",
    "./node": "./src/index.node.js",
    "./web": "./src/index.web.js",
    "./define": "./src/define.js",
    "./components": {
      "types": "./dist-types/src/index.components.d.ts",
      "default": "./src/index.components.js"
    }
  },
  "scripts": {
    "analyze": "cem analyze --litelement",
    "debug": "cd ../../ && npm run debug -- --group search",
    "test": "npm run test:node && npm run test:web",
    "test:node": "mocha --require ../../scripts/testMochaGlobalHooks.js test-node/**/*.test.js test-node/*.test.js",
    "test:watch": "onchange 'src/**/*.{js,cjs}' 'test-node/**/*.js' -- npm run test:node",
    "test:web": "cd ../../ && npm run test:web -- --group search"
  },
  "files": [
    "*.js",
    "assets",
    "dist",
    "dist-types",
    "preset",
    "src"
  ],
  "keywords": [
    "rocket",
    "search"
  ],
  "dependencies": {
    "@rocket/engine": "^0.1.0-alpha.22",
    "minisearch": "^3.0.2",
    "plugins-manager": "^0.3.0",
    "sax-wasm": "^2.0.0"
  },
  "devDependencies": {
    "@rocket/cli": "^0.20.0-alpha.16"
  },
  "customElements": "custom-elements.json"
}
